<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AllOne" parent="Theme.AppCompat.Light">
        <item name="android:colorPrimary">@android:color/holo_orange_light</item>
        <item name="android:colorPrimaryDark">@android:color/holo_orange_dark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:background">@android:color/transparent</item>

        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorLink">@color/theme_accent</item>

    </style>

    <style name="CollapsedAppBar" parent="@android:style/TextAppearance">
        <item name="android:textSize">22sp</item>

    </style>


    <style name="ThemeTransparentColor" parent="Theme.AppCompat">
        <item name="android:background">@android:color/transparent</item>
    </style>


    <style name="ThemeStory" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorLink">@color/theme_accent</item>
    </style>


</resources>